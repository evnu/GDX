<% content_for :title do %>
  Upload
<% end %>

<% content_for :content do %>
  <p id="notice"><%= notice %></p>

  <p>
  <b>Name:</b>
  <%= @upload.name %>
  </p>

  <p>
  <b>Description:</b>
  <%= raw @upload.description %>
  </p>

  <p>
  <b>Tags.</b>
  <%= @upload.tag_list %>
  </p>


  <%= link_to 'Edit', edit_upload_path(@upload) %> |
  <%= link_to 'Back', uploads_path %>
<% end %>
